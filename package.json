{
  "name": "dotfiles",
  "private": true,
  "scripts": {
    "start": "npm run sync",
    "sync": "node ./scripts/sync.js -df",
    "postsync": "node ./scripts/desensitize.js",
    "restore": "npm run safe-restore",
    "safe-restore": "node ./scripts/safe-restore.js",
    "unsafe_restore": "node ./scripts/sync.js -drf",
