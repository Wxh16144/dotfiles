[
  {
    "version" : 2
  },
  {
    "action" : {
      "args" : "commit --amend --no-edit --no-verify",
      "path" : "${git}",
      "showOutput" : false,
      "type" : "process",
      "waitForExit" : true
    },
    "name" : "?Amend",
    "target" : "repository"
  },
  {
    "action" : {
      "script" : "open ${repo:path}",
      "showOutput" : false,
      "type" : "sh",
      "waitForExit" : true
    },
    "name" : "?Open in Finder",
    "target" : "repository"
  },
  {
    "action" : {
      "script" : "open -a SourceTree ${repo:path}",
      "showOutput" : false,
      "type" : "sh",
      "waitForExit" : true
    },
    "name" : "?Open in Sourcetree",
    "target" : "repository"
  },
  {
    "action" : {
      "args" : "${repo:path} --disable-extensions",
      "path" : "\/usr\/local\/bin\/code",
      "showOutput" : false,
      "type" : "process",
      "waitForExit" : true
    },
    "name" : "?Open in VSCode (Disable Eextensions)",
    "target" : "repository"
  },
  {
    "action" : {
      "script" : "${git} for-each-ref \\\n\t--sort=-committerdate \\\n\t--count=10 \\\n\t--format='%(color:green)%(refname:short)%(color:reset) %(committerdate:relative) %(authorname)' \\\n\trefs\/heads;\n\n",
      "showOutput" : true,
      "type" : "sh",
      "waitForExit" : true
    },
    "name" : "?Recent-Branches ï¼ˆ10 items",
    "target" : "repository"
  },
  {
    "action" : {
      "args" : "checkout @{-1}",
      "path" : "${git}",
      "showOutput" : false,
      "type" : "process",
      "waitForExit" : true
    },
    "name" : "?Switch Previous Branch",
    "target" : "repository"
  },
  {
    "action" : {
      "args" : "branch -a -r --contains $SHA",
      "path" : "${git}",
      "showOutput" : true,
      "type" : "process",
      "waitForExit" : true
    },
    "name" : "?Which branches contain this commit",
    "target" : "revision"
  }
]